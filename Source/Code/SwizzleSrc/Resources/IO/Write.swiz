extend IO {
    func print(args: Any...) {
        let str = __description(args, " ");
        let data = str::data(self._encoding);
        __handle.write(data);
    }
    func printLn(args: Any...) {
        var str = __description(args, " ");
        str::add("\n");
        let data = str::data(self._encoding);
        __handle.write(data);
    }
}
